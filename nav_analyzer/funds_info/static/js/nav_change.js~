$(function() {
    var get_nav_change = (function ($) {
        var target_element = $("#nav-change-wrapper");
        var stocks_data_wrapper = $("#stocks-data-wrapper");
        var stocks_li = $("#stocks-data-wrapper li");
        var stock_count = $("#stocks-data-wrapper li").length;
        var result_span = $("#result");
        var url = "/funds/fund/nav_change";
        var slug = target_element.attr("data-attr");
        var start = 0;
        var end = 10;

        var get_stock_classes = function() {
            return stocks_li.slice(start, end);
        };

        var fetch_nav_change = function() {
            console.log(url);
            $.ajax({
                url: url,
                type: 'GET',
                contentType: 'application/json',
                data: {
                    list: slug
                },
                dataType: 'json',
                success: function(response) {
                    console.log(response);
                    var ch = response.ch;
                    result_span.show();
                    result_span.html(ch);
                },
                error: function(response) {
                    console.log(response)
                }
            });

            /*
            fetch(url, req_data).then(function(response) {
                return response;
            }).then(function(response) {
                console.log(response);
            });
            */
        };

        var init = function() {
            target_element.on("click", function() {
                fetch_nav_change();
            });
        };

        return {
            init: init
        }

    })(jQuery);
    get_nav_change.init();
});
